				INSERT INTO detalle_credito(cod_notc, pro_cod, id_depo, cantidad, precio, subtotal,exentas, iva5, iva10)
				 VALUES(vidnota, vproducto,vdeposito, vcantidad,vprecio, vsubtotal, vexentas, viva5, viva10);
