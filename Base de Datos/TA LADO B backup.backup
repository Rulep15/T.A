			ELSE
				DELETE FROM detalle_debito WHERE id_moti = vidmotivo AND id_debito= vidnota;
				mensaje = 'NOTICIA_/_SE HA QUITADO EL REGISTRO DEL DETALLE';
				
			END IF;
		END IF;	
		RETURN mensaje;
	END;
$$;


