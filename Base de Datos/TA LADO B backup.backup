				mensaje = 'NOTICIA_/_NOTA DE CREDITO CONFIRMADO*nota_credito_index'; 
			ELSE
				mensaje = 'ERROR_/_DEBE CARGAR DETALLES EN LA NOTA DE CREDITO*nota_credito_index'; 
			END IF;
			
		END IF;
		RETURN mensaje;
